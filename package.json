{
    "name": "fontawesome-css-class-name-auto-complete",
    "displayName": "FontAwesome 5 CSS class name auto-complete",
    "description": "Provides FontAwesome 5.0.12 CSS class names as autocompletion items for all languages.",
    "version": "0.0.1",
    "publisher": "Janne252",
    "license": "MIT",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other", "Providers"
    ],
    "keywords": [
        "FontAwesome", "FontAwesome 5", "class names", "auto-complete"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
